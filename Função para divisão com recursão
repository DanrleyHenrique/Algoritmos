#include<stdio.h>;
//c√≥digo precisando ser debugado

int divide(int dividendo, int divisor){
	
    int sub;
    int contador;
    sub = dividendo-divisor;
    contador++;
    if(sub == 0)
 	return contador;
    divide(sub, divisor);
}


int main(void){

	// Disable stdout buffering
    setvbuf(stdout, NULL, _IONBF, 0);       
       
	int divisor = 0;
	int dividendo = 0;
	int resultado = 0;

    puts("Entre com o dividendo");
    scanf("%d",&dividendo);
       
    puts ("Entre com o divisor");
    scanf("%d",&divisor);

    
	printf("%d divido por %d = %d\n", dividendo, divisor, divide(dividendo, divisor));
	
	return (0);
   
}       
          
