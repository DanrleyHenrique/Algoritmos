#include<stdio.h>;

//cÃ³digo a reparar
int divide(int dividendo, int divisor){
    int sub, x; 
    int contador = 0;
    
    sub = dividendo-divisor;
    contador++;
    
    if(sub > divisor)
    	divide(sub, divisor);
    if(sub == 0)
    	return contador;
   
    }


int main(void){

	// Disable stdout buffering
    setvbuf(stdout, NULL, _IONBF, 0);       
       
	int divisor = 0;
	int dividendo = 0;
	int resultado = 0;

    puts("Entre com o dividendo");
    scanf("%d",&dividendo);
       
    puts ("Entre com o divisor");
    scanf("%d",&divisor);
    
   	resultado = divide(dividendo, divisor);
    
	printf("%d divido por %d = %d\n", dividendo, divisor, resultado);
	
	return (0);
   
}       
          
